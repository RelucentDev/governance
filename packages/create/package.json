    "swc": "swc ./src/ -d ./dist/",
    "tsc": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "minimist": "^1.2.8",
    "plop": "^4.0.0",
    "pluralize": "^8.0.0",
    "title": "^3.5.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.96",
    "@types/pluralize": "^0.0.33",
    "@types/title": "^3.4.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
